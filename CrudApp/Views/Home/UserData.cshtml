@model List<CrudApp.Models.LoginClass>
@{
    ViewBag.Title = "UserData";
}

<h2>Users Data</h2>

<table class="table table-bordered table-responsive">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Password</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Password</td>
        </tr>
    }
</table>